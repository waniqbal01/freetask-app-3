{
  "name": "freetask-api",
  "version": "0.0.1",
  "description": "Freetask API built with NestJS and Prisma",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.prisma\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "prisma:generate": "npm exec prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "prisma:seed": "ts-node src/seed.ts",
    "postinstall": "chmod +x node_modules/.bin/prisma node_modules/prisma/build/index.js 2>/dev/null || true && prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^5.18.0",
    "bcrypt": "^5.1.1",
    "prisma": "^5.18.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "file-type": "^20.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "swagger-ui-dist": "^5.30.3",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/passport-jwt": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  }
}